package pkg;


pure func test(s [20]int, n int) (res int) {
    return test_internal(s,n,0)
}

requires i>=0
requires i<20
pure func test_internal(s [20]int, needle int, i int) (res int) {
    found:=0
    if s[i]==needle {
        found++
    }
    i=i+1
    if i==len(s){
        return found
    }
    return test_internal(s,needle + found,i)
}
